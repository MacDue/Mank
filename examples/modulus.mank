typedef Nat = i32;

fun max: Nat (a: Nat, b: Nat) {
  if a > b { a } else { b }
}

fun mu: Nat (f: \|\Nat -> Nat| -> Nat, a: \Nat -> Nat) {
  r := vec!([0]);
  b := \n -> {
    r[0] = max(n, r[0]);
    a(n)
  }
  _ := f(b);
  r[0]
}

proc main {
  result := mu(\a -> { a(a(10)) }, \n -> { n * n });
  println!("- : Nat = {result}", int_to_string(result));
}
